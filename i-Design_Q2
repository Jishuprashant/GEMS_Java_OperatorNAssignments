// package com.java;
import java.util.*;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter the number :");
		int num = sc.nextInt();
		int check = 1;
		
		if( (num & check) == 1) {
			System.out.println("Odd");
		}
		else
			System.out.println("Even");
		
		sc.close();
	}

}
