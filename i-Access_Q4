package com.java;
import java.util.*;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter x");
		int x = sc.nextInt();
		
		int total = x * 3;
		
		System.out.println(total+" people were there in total");
		
		sc.close();
	}

}
